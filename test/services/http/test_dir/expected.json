{
   "$jason" : {
      "head" : {
         "templates" : {
            "body" : {
               "style" : {
                  "background" : "green"
               },
               "sections" : [
                  {
                     "type" : "horizontal",
                     "items" : {
                        "{{#each views}}" : {
                           "type" : "vertical",
                           "style" : {
                              "padding" : "5",
                              "distribution" : "equalspacing",
                              "background" : [
                                 {
                                    "{{#if (active)}}" : "red"
                                 },
                                 {
                                    "{{#else}}" : "transparent"
                                 }
                              ],
                              "spacing" : "13",
                              "height" : "120",
                              "align" : "center"
                           },
                           "components" : [
                              {
                                 "url" : "http://localhost:1234/views/appid2/favicon.png",
                                 "type" : "image",
                                 "style" : {
                                    "corner_radius" : "5",
                                    "width" : "76",
                                    "height" : "76"
                                 }
                              },
                              {
                                 "text" : "{{name}}",
                                 "style" : {
                                    "size" : "12"
                                 },
                                 "type" : "label"
                              }
                           ]
                        }
                     }
                  },
                  {
                     "url" : "http://localhost:1234/views/appid2/x",
                     "hello" : "world"
                  }
               ],
               "header" : {
                  "title" : {
                     "url" : "https://raw.githubusercontent.com/Jasonette/Instagram-UI-example/master/images/logo.png",
                     "style" : {
                        "height" : "27",
                        "width" : "94"
                     },
                     "type" : "image"
                  },
                  "style" : {
                     "shy" : "true"
                  },
                  "menu" : {
                     "image" : "https://raw.githubusercontent.com/Jasonette/Instagram-UI-example/master/images/box.png"
                  }
               }
            }
         },
         "actions" : {
            "rpc-name" : {
               "type" : "$external.invoke",
               "options" : {
                  "method" : "com.srivatsaniyer.weaveremote.jasonette.Messaging.rpc",
                  "data" : {
                     "apis" : {
                        "api1" : {
                           "description" : "desc2",
                           "request_schema" : {
                              "required" : [
                                 "command",
                                 "id"
                              ],
                              "additionalProperties" : false,
                              "type" : "object",
                              "properties" : {
                                 "id" : {
                                    "type" : "string"
                                 },
                                 "command" : {
                                    "enum" : [
                                       "api1"
                                    ]
                                 }
                              }
                           },
                           "response_schema" : {},
                           "name" : "api1",
                           "args" : [],
                           "kwargs" : {}
                        }
                     },
                     "response_queue" : "/components/appid2/rpcs/325fddc4-d2fc-483d-b2b2-507f1fe284f9/response",
                     "request_queue" : "/components/appid2/rpcs/325fddc4-d2fc-483d-b2b2-507f1fe284f9/request"
                  }
               }
            }
         },
         "data" : {
            "views" : [
               {
                  "name" : "Name",
                  "app_id" : "appid2",
                  "active" : false,
                  "view" : {
                     "hello" : "world",
                     "url" : "http://localhost:1234/views/appid2/x"
                  }
               }
            ]
         }
      }
   }
}
