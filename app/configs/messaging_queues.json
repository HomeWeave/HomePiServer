{
    "queues": [
        {
            "queue_name": "/tv/command",
            "request_schema": {
                "type": "object",
                "properties": {
                    "command": {
                        "description": "The command to send to TV",
                        "enum": ["POWER", "LEFT", "RIGHT", "UP", "DOWN"]
                    }
                },
                "required": ["command"]
            },
            "response_schema": {
                "type": "object",
                "properties": {
                    "result": {
                        "description": "The result of the command to send to TV",
                        "enum": ["NETWORK-ERROR", "OK", "UNKNOWN-ERROR"]
                    }
                },
                "required": ["result"]
            },
            "online": true,
            "timeout": 15
        },
        {
            "queue_name": "/phone/apps",
            "request_schema": {
                "type": "object"
            },
            "response_schema": {
                "type": "object",
                "properties": {
                    "result": {
                        "description": "The result of the command to send to TV",
                        "enum": ["NETWORK-ERROR", "OK", "UNKNOWN-ERROR"]
                    }
                },
                "required": ["result"]
            },
            "queue_type": "sticky",
            "timeout": 15
        }
    ]
}
